{
    "spritesheet": {
        {{#spritesheet}}
        "width": {{width}},
        "height": {{height}},
        "url": "static/spritesheet.png"
        {{/spritesheet}}
    },
    "sprites": {
        {{#sprites}}
        "{{name}}": {
            "x": {{x}},
            "y": {{y}},
            "width": {{width}},
            "height": {{height}}
        }{{#unless @last}},{{/unless}}
        {{/sprites}}
    }
}